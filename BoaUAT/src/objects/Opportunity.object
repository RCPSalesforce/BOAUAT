    <validationRules>
        <fullName>Capture_reason_closed_lost</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
ISPICKVAL( StageName  ,&quot;Closed Lost&quot;), 
ISBLANK( Closed_Lost_Reason__c ) 
)</errorConditionFormula>
        <errorMessage>Please enter text in the field closed lost reason</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Opportunity_Stage_rule</fullName>
        <active>false</active>
        <errorConditionFormula>AND( ISPICKVAL( StageName ,&apos;Closed Won&apos;),
   OR(
       ISBLANK(Onboarding_Target_Start_Date__c),
         ISBLANK( Onboarding_Target_Closure_Date__c )  
 )
)</errorConditionFormula>
        <errorMessage>If the stagename is closed won,Onboarding target start date,Onboarding target closure date is  mandetory.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Opportunity_type_Rule</fullName>
        <active>false</active>
        <errorConditionFormula>AND( ISPICKVAL( Type , &apos;New Business&apos;) ,
OR(
ISBLANK(Onboarding_Target_Start_Date__c),
         ISBLANK( Onboarding_Target_Closure_Date__c ),
 ISBLANK( Onboarding_Point_of_Contact__c) 
)

)</errorConditionFormula>
        <errorMessage>If Order Type is New Business, Onboarding Target Start Date,Onboarding Target Closure Date and Onbording Point of Contact are Mandatory.</errorMessage>
    </validationRules>
