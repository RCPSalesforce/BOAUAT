    <validationRules>
        <fullName>Asset_Scheduler</fullName>
        <active>true</active>
        <errorConditionFormula>IF(Schedule_Next_Job__c,IF(OR(ISNULL(Schedule_Until__c),ISBLANK(TEXT(Schedule_On__c))),true,false),false)</errorConditionFormula>
        <errorMessage>When the Schedule Next Job is checked, then the &apos; Schedule Until &apos; and &apos; Schedule On &apos; field is mandatory</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Schedule_Days_After</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISNULL(Days__c),TEXT(Schedule_On__c)=&apos;Days After&apos;)</errorConditionFormula>
        <errorDisplayField>Days__c</errorDisplayField>
        <errorMessage>When you will choose Days After then Days is mandatory</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Schedule_Not_Days_After</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT(ISNULL(Days__c)),TEXT(Schedule_On__c)!=&apos;Days After&apos;)</errorConditionFormula>
        <errorDisplayField>Days__c</errorDisplayField>
        <errorMessage>Do not give Days values. It&apos;s only mandatory for &quot;Days After&quot;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Schedule_Until_Day_Check</fullName>
        <active>true</active>
        <errorConditionFormula>Schedule_Until__c&lt;TODAY()</errorConditionFormula>
        <errorDisplayField>Schedule_Until__c</errorDisplayField>
        <errorMessage>Schedule Until must be equal or greater than today</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Year_Format</fullName>
        <active>true</active>
        <errorConditionFormula>IF(NOT(ISBLANK(Year__c)),IF(AND(LEN(Year__c)=4,isNumber(Year__c)),false,true),false)</errorConditionFormula>
        <errorDisplayField>Year__c</errorDisplayField>
        <errorMessage>Enter the correct year format YYYY</errorMessage>
    </validationRules>
