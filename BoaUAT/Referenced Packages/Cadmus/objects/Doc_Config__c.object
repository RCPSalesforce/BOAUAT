var arr = document.querySelector(&apos;#bodyCell&apos;).querySelectorAll(&apos;input.btn&apos;);
for (var key in arr) { 
	if (arr.hasOwnProperty(key)) { 
		arr[key].disabled = true;
		arr[key].className += &apos; btnDisabled&apos;;
		if(arr[key].name.indexOf(&apos;cadmus_sync&apos;) &gt;= 0) {
			arr[key].value = &apos;Syncing...&apos;;
		}
	}
}
*/
{!REQUIRESCRIPT(&quot;/soap/ajax/38.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/38.0/apex.js&quot;)}

try { 
  var syncResult = sforce.apex.execute(&quot;cadmus.DocumentConfigHandler&quot;,&quot;sync&quot;,{docConfigId:&quot;{!Doc_Config__c.Id}&quot;});

  location.reload();
} catch(e) { 
  alert(e.faultstring); 
}</url>
