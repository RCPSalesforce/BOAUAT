    <validationRules>
        <fullName>Validate_Group_By</fullName>
        <active>false</active>
        <description>If Group By is checked, make sure Quote line item API name is for Product object.</description>
        <errorConditionFormula>AND (
  Group_By__c,
  NOT BEGINS( Quote_line_item_API_name__c , &apos;PricebookEntry.Product2.&apos;) 
)</errorConditionFormula>
        <errorDisplayField>Quote_line_item_API_name__c</errorDisplayField>
        <errorMessage>You can group items only by product field. 
For e.g. &quot;PricebookEntry.Product2.Family&quot; OR
&quot;PricebookEntry.Product2.Custom_Field__c&quot;</errorMessage>
    </validationRules>
